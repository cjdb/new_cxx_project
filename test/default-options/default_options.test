RUN: %rm %t/default_options
RUN: %new-project %t/default_options --author='new-c++-project Test Suite'
RUN: diff --unified --color --recursive %S/expected-output %t/default_options
RUN: cp %S/changes/source/* %t/default_options/source/.

RUN: %rm %t/build %t/install
RUN: cmake -S %t/default_options -B %t/build -GNinja -DCMAKE_BUILD_TYPE=Debug -DCMAKE_INSTALL_PREFIX=%t/install 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-CMAKE,CHECK-CMAKE-DEBUG --match-full-lines
RUN: ninja -j1 -v -C %t/build 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-NINJA-DEBUG --match-full-lines
RUN: ctest --test-dir %t/build --output-on-failure 2>&1 | %{replace_path} | FileCheck %s --check-prefix=CHECK-CTEST --match-full-lines

RUN: %rm %t/build %t/install
RUN: cmake -S %t/default_options -B %t/build -GNinja -DCMAKE_BUILD_TYPE=Release 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-CMAKE,CHECK-CMAKE-RELEASE --match-full-lines
RUN: ninja -j1 -v -C %t/build 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-NINJA-RELEASE --match-full-lines
RUN: ctest --test-dir %t/build --output-on-failure 2>&1 | %{replace_path} | FileCheck %s --check-prefix=CHECK-CTEST --match-full-lines

RUN: %rm %t/build %t/install
RUN: cmake -S %t/default_options -B %t/build -GNinja -DCMAKE_BUILD_TYPE=MinSizeRel 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-CMAKE,CHECK-CMAKE-MINSIZEREL --match-full-lines
RUN: ninja -j1 -v -C %t/build 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-NINJA-MINSIZEREL --match-full-lines
RUN: ctest --test-dir %t/build --output-on-failure 2>&1 | %{replace_path} | FileCheck %s --check-prefix=CHECK-CTEST --match-full-lines

RUN: %rm %t/build %t/install
RUN: cmake -S %t/default_options -B %t/build -GNinja -DCMAKE_BUILD_TYPE=RelWithDebInfo 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-CMAKE,CHECK-CMAKE-RELWITHDEBINFO --match-full-lines
RUN: ninja -j1 -v -C %t/build 2>&1 | %{replace_path} | FileCheck %s --check-prefixes=CHECK-NINJA-RELWITHDEBINFO --match-full-lines
